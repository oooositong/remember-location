{
  "name": "remember-location",
  "version": "1.0.3",
  "description": "A simple tool to help your website remember the historical location and relocate to it when revisit the same page",
  "main": "./dist/rollup.umd.js",
  "directories": {
    "test": "test"
  },
  "scripts": {
    "serve": "serve .",
    "webpack:umd": "webpack --config webpack.umd.config.js",
    "webpack:cjs": "webpack --config webpack.cjs.config.js",
    "webpack:esm": "webpack --config webpack.esm.config.js",
    "webpack": "npm run webpack:umd && npm run webpack:cjs && npm run webpack:esm",
    "rollup": "rollup --config rollup.config.js"
  },
  "keywords": ["remember location"],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "lodash": "^4.17.21",
    "serve": "^12.0.0"
  },
  "devDependencies": {
    "@babel/core": "^7.14.8",
    "@babel/preset-env": "^7.14.9",
    "@rollup/plugin-babel": "^5.3.0",
    "@rollup/plugin-commonjs": "^20.0.0",
    "@rollup/plugin-node-resolve": "^13.0.4",
    "webpack": "^5.47.1",
    "webpack-bundle-analyzer": "^4.4.2",
    "webpack-cli": "^4.7.2"
  }
}
